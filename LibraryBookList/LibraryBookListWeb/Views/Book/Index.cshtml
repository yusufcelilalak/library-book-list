@model IEnumerable<Book>
@{
    ViewData["Title"] = "Index";
}

<h1>Index Book</h1>

<table class="table table-bordered table-striped mx-auto mt-5" style="width:70%" >
    <thead>
        <tr>
            <th>
                ID
            </th>
            <th>
                Title
            </th>
            <th>
                Author
            </th>
            <th>
                Category
            </th>
            <th>
                Page
            </th>
            <th>
                Added Date
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach(var obj in Model)
        {
            <tr>
                <td>
                    @obj.ID
                </td>
                <td>
                    @obj.Title
                </td>
                <td>
                    @obj.Author
                </td>
                <td>
                    @obj.Category
                </td>
                <td>
                    @obj.Page
                </td>
                <td>
                    @obj.AddedDate
                </td>
            </tr>
        }
    </tbody>

</table>


